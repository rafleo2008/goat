{
  "name": "footpath_width",
  "rules": [
    {
      "name": "noData",
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#999999",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "0m - 1m",
      "filter": [
        "&&",
        [
          ">=",
          "width",
          "0"
        ],
        [
          "<=",
          "width",
          "1"
        ]
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#f2ef24",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "1m - 2m",
      "filter": [
        "&&",
        [
          ">",
          "width",
          "1"
        ],
        [
          "<=",
          "width",
          "2"
        ]
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#2ed558",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "2m - 3m",
      "filter": [
        "&&",
        [
          ">",
          "width",
          "2"
        ],
        [
          "<=",
          "width",
          "3"
        ]
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#2aaa4a",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "3m - 4m",
      "filter": [
        "&&",
        [
          ">",
          "width",
          "3"
        ],
        [
          "<=",
          "width",
          "4"
        ]
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#2a924a",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "> 4m",
      "filter": [
        "&&",
        [
          ">",
          "width",
          "4"
        ],
        [
          "<=",
          "width",
          "35"
        ]
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#197836",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "livingStreet",
      "filter": [
        "==",
        "highway",
        "living_street"
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#27d9d9",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "noSidewalk",
      "filter": [
        "==",
        "sidewalk",
        "no"
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#ff0d01",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    }
  ]
}